<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/decorators">
	<decorator name="HeaderAndFooter" page="login.jsp">
		<pattern>/*</pattern>
		<pattern>/player/character.jsp</pattern>
	</decorator>

	<decorator name="Character" page="infinite.jsp">
		<pattern>/player/*</pattern>
	</decorator>

	<decorator name="Admin" page="admin.jsp">
		<pattern>/admin/*</pattern>
	</decorator>



	<excludes>
		<pattern>*monsterXML*</pattern>
		<pattern>*css*</pattern>
		<pattern>*prepare*</pattern>
	</excludes>


</decorators>

